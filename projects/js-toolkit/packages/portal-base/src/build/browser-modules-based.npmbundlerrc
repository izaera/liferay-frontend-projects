{
	"dump-report": true,
	"create-jar": {
		"output-dir": "dist"
	},
	"exclude": {
		"*": true
	},
	"ignore": [
		"**/*"
	],
	"rules": [
		{
			"test": "\\.css$",
			"use": ["css-loader"]
		},
		{
			"exclude": "node_modules",
			"test": ".*/[^_][^/]+\\.scss$",
			"use": [
				{
					"loader": "css-loader",
					"options": {
						"emitCssFile": false,
						"extension": ".css"
					}
				}
			]
		}
	],
	"sources": ["src"]
}

